    "import glob\n",
    "from PIL import Image\n",
    "import os\n",
    "import cv2\n",
    "\n",
    "files = glob.glob(\"image/*.jpg\")\n",
    "for file in files:\n",
    "    img = cv2.imread(file)\n",
    "    img[:,:,:] = 0\n",
    "    filename = os.path.basename(file)\n",
    "    \n",
    "    #print(filename)\n",
    "    #print(img.shape)\n",
    "    cv2.imwrite(\"./\"+filename, img)"
   ]
