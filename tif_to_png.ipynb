   "source": [
    "from PIL import Image\n",
    "import os\n",
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "def tif_to_png(img_path):\n",
    "    img = Image.open(img_path)\n",
    "    img.save(img_path.replace(\".tif\",\".png\"), quality=95)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "1a6aa116",
   "metadata": {},
   "outputs": [],
   "source": [
    "#main\n",
    "from glob import glob\n",
    "files = glob(\"./*.tif\")\n",
    "for file in files:\n",
    "    tif_to_png(file)"
   ]
  },
