    "import glob\n",
    "import os\n",
    "files = glob.glob(\"image/*.png\")\n",
    "\n",
    "for file in files:\n",
    "    img_file_name = os.path.basename(file)\n",
    "    mask_path = os.path.join(\"mask\", img_file_name)\n",
    "    if os.path.isfile(mask_path)==False:\n",
    "        os.remove(file)"
